<?xml version="1.0"?>
<config>
	<modules>
		<Auctionmaid_Productmatrix>
			<active>true</active>
			<codePool>local</codePool>
		</Auctionmaid_Productmatrix>
	</modules>
	<global>
	<models>
		 <adminhtml>
			<rewrite>
				<system_config_source_shipping_productmatrix>Auctionmaid_Productmatrix_Adminhtml_Model_System_Config_Source_Shipping_Productmatrix</system_config_source_shipping_productmatrix>
			</rewrite>
		  </adminhtml>
	</models>
	<models>
		 <adminhtml>
			<rewrite>
				<system_config_backend_shipping_productmatrix>Auctionmaid_Productmatrix_Adminhtml_Model_System_Config_Backend_Shipping_Productmatrix</system_config_backend_shipping_productmatrix>
			</rewrite>
		  </adminhtml>
	</models>
	<models>
		<shipping>
			<rewrite>
				<carrier_productmatrix>Auctionmaid_Productmatrix_Model_Carrier_Productmatrix</carrier_productmatrix>
				<mysql4_carrier_productmatrix>Auctionmaid_Productmatrix_Model_Mysql4_Carrier_Productmatrix</mysql4_carrier_productmatrix>
			</rewrite>
		</shipping>
	</models>
	</global>
</config>
